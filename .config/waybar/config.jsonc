{
    "layer": "top",
"position": "top",
    "height": 30,
    "spacing": 0,
    "modules-left": ["custom/power", "hyprland/workspaces", "hyprland/submap", "custom/media"],
    "modules-center": ["hyprland/window"],
    "modules-right": ["pulseaudio", "network", "cpu", "memory", "clock", "tray", "custom/swaync"],

	"custom/power": {
		"format": "Û∞£á",
		"tooltip-format": "Power",
		"on-click": "powermenu.sh"
	},

    "hyprland/workspaces": {
        "all-outputs": true,
        "format": "{name} {icon} ",
        "format-icons": {
            "1": "ÔÇ¨",
            "2": "ÔÅÑ",
            "3": "ÔÑ†",
            "4": "Ôâ∫",
            "5": "ÔáÄ",
            "urgent": "",
            "focused": "",
            "default": ""
        }
    },

	"hyprland/window": {
		"icon": true,
		"icon-size": 22
	},

    "hyprland/submap": {
        "format": "<span style=\"italic\">{}</span>"
    },

    "tray": {
        "spacing": 10
    },

    "clock": {
        "format": " {:%H:%M}",
        "format-alt": " {:%d-%m-%Y}",
		"tooltip-format": "<big>{:%B %Y}</big><tt><small>{calendar}</small></tt>",
        "calendar": {
                    "mode"          : "month",
                    "weeks-pos"     : "right",
                    "on-scroll"     : 1,
                    "format": {
                              "months":     "",
                              "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                              "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                              "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                              "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
                              }
                    },
    },

    "cpu": {
        "format": " Ôãõ {usage}%",
        "tooltip": true
    },

    "memory": {
        "format": " Óâ¶ {}%"
    },

    "network": {
        "format-wifi": " {icon} {ifname}",
        "format-ethernet": " Û∞õ≥ {ifname}",
        "format-linked": " {ifname} ",
        "format-disconnected": " Û∞≤õ ",
		"format-icons": ["Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"],
        "tooltip-format": "{ifname}: {ipaddr}/{cidr}\nUp: {bandwidthUpBits}\nDown: {bandwidthDownBits}"
    },

    "pulseaudio": {
        "format": "{icon}  {volume}%",
        "format-bluetooth": "{icon}  {volume}%",
        "format-bluetooth-muted": "{icon}",
        "format-muted": "Óª®",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "Óπô",
            "headset": "Óπô",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },

    "custom/media": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "",
            "default": "üéú"
        },
        "escape": true,
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null"
    },

	"custom/swaync": {
		"tooltip": false,
		"format": "{} {icon}",
		"format-icons": {
			"notification": "Û∞Çö<span foreground='red'><sup>ÔëÑ</sup></span>",
			"none": "Û∞Çö",
			"dnd-notification": "Û∞Çõ<span foreground='red'><sup>ÔëÑ</sup></span>",
			"dnd-none": "Û∞Çõ",
			"inhibited-notification": "Û∞Çö<span foreground='red'><sup>ÔëÑ</sup></span>",
			"inhibited-none": "Û∞Çö",
			"dnd-inhibited-notification": "Û∞Çõ<span foreground='red'><sup>ÔëÑ</sup></span>",
			"dnd-inhibited-none": "Û∞Çõ"
		},
		"return-type": "json",
		"exec-if": "which swaync-client",
		"exec": "swaync-client -swb",
		"on-click": "swaync-client -t -sw",
		"on-click-right": "swaync-client -d -sw",
		"escape": true
	},
}
